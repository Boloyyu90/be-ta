{
  "name": "backend-tryout-system-with-proctoring",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --exec tsx src/server.ts",
    "build": "tsc && tsc-alias",
    "start": "node dist/server.js",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"prisma/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"prisma/**/*.ts\"",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:prod": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "prisma:seed": "tsx prisma/seed.ts",
    "prisma:reset": "prisma migrate reset --force",
    "db:push": "prisma db push",
    "test": "echo \"Error: no test specified\" && exit 0",
    "prepare": "npx husky install"
  },
  "keywords": [
    "Express",
    "Typescript",
    "Prisma",
    "PostgreSQL"
  ],
  "author": "Your Name",
  "license": "MIT",
  "description": "Backend for online tryout system with proctoring",
  "devDependencies": {
    "@tsconfig/node20": "^20.1.4",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.13.9",
    "@types/pino": "^7.0.4",
    "@types/pino-http": "^5.8.4",
    "@types/pino-pretty": "^4.7.5",
    "@types/supertest": "^6.0.2",
    "@vitest/coverage-v8": "^3.0.8",
    "nodemon": "^3.1.9",
    "prettier": "^3.5.3",
    "prisma": "^6.4.1",
    "supertest": "^7.0.0",
    "tsc-alias": "^1.8.11",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "vitest": "^3.0.8"
  },
  "dependencies": {
    "@prisma/client": "^6.4.1",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^3.0.2",
